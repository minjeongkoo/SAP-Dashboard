{
    "sap.app": {
		"id": "openUI5",
        "applicationVersion": {
        	"version": "1.0.0"
        },
        "i18n": "i18n/i18n.properties"
    },
    "sap.ui5": {
		"rootView": {
			"viewName": "OpenUI5.view.frame.Layout",
			"type": "XML",
			"async": true
		},
        "dependencies": {
        	"libs": {
        		"sap.ui.layout": {},
                "sap.m": {}
            }
        }, 
        "resources": {
        	"css" : [{ "uri": "css/style.css", "id": "css" }]
        },
        "models": {
        	"i18n": {
        		"type": "sap.ui.model.resource.ResourceModel",
                "settings": {
				"bundleName": "OpenUI5.i18n.i18n"
				}
            }
        },
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"controlId": "centerView",
				"controlAggregation": "pages",
				"transition": "show",
				"async": true
			},
			"routes": [{ "name": "contents1", "pattern": ""         , "target": "contents1" },
			           { "name": "contents2", "pattern": "contents2", "target": "contents2" },
			           { "name": "contents3", "pattern": "contents3", "target": "contents3" }],
			"targets": {
				"contents1": {
					"viewPath": "OpenUI5.view.contents",
					"viewName": "Contents1",
					"viewLevel": 1,
					"viewId": "contents1"
				},
				"contents2": {
					"viewPath": "OpenUI5.view.contents",
					"viewName": "Contents2",
					"viewLevel": 1,
					"viewId": "contents2"
				},
                "contents3": {
                    "viewPath": "OpenUI5.view.contents",
                    "viewName": "Contents3",
                    "viewLevel": 1,
                    "viewId": "contents3"
                }
			}
		}
    }
}